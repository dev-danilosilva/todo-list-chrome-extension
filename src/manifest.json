{
  "manifest_version": 2,
  "name": "Minhas Tarefas",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["todo/style.css"],
      "js": ["todo/app.js"]
    }
  ],
  "browser_action": {
    "default_icon": "todo/assets/icons/tarefa.png",
    "default_title": "Tarefas",
    "default_popup": "todo/index.html"
  }
}
